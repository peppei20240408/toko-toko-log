=begin %>
  <div class="field">
    <%= f.label :images, "トコLogの画像たち" %><br />
    <%= f.file_field :images, id:"prototype_image" %>
  </div>
<% 
=end%>

  <div class="actions">
    <%= f.submit "保存する", class: :form__btn  %>
  </div>
<% end %>